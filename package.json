		"chai-bn": "^0.3.0",
		"eslint": "^6.5.1",
		"eslint-config-standard": "^14.1.1",
		"eslint-plugin-import": "^2.20.0",
		"eslint-plugin-mocha-no-only": "^1.1.0",
		"eslint-plugin-node": "^10.0.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^4.0.1",
		"eth-gas-reporter": "*",
		"eth-sig-util": "^3.0.0",
		"ethers": "*",
		"ganache-time-traveler": "*",
		"global": "^4.4.0",
		"graphlib": "^2.1.8",
